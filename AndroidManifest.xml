<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtest.austin"
    android:versionCode="7"
    android:versionName="1.0.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="11" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/math"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
        <activity android:name="com.androidtest.austin.Splash" 
            android:label="@string/app_name"
            android:screenOrientation="portrait">
			<intent-filter>
  				<action android:name="android.intent.action.MAIN" /> 
  				<category android:name="android.intent.category.LAUNCHER" /> 
  			</intent-filter>
  		</activity>
		<activity android:name="com.androidtest.austin.MenuActivity"
		    android:label="@string/app_name"
		    android:screenOrientation="portrait">
			<intent-filter>
  				<action android:name="com.androidtest.austin.MENU" /> 
  				<category android:name="android.intent.category.DEFAULT" /> 
  			</intent-filter>
  		</activity>
		<activity android:name="com.androidtest.austin.HowToPlayActivity"
		    android:label="@string/app_name"
		    android:screenOrientation="portrait">
			<intent-filter>
  				<action android:name="com.androidtest.austin.HOWTO" /> 
  				<category android:name="android.intent.category.DEFAULT" /> 
  			</intent-filter>
  		</activity>
        <activity
            android:name="com.androidtest.austin.MainActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.MAINACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidtest.austin.PauseMenuActivity"
            android:label="Paused" 
            android:theme="@android:style/Theme.Dialog"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.PAUSEMENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidtest.austin.AnswerInputDialogActivity"
            android:label="Your Answer" 
            android:theme="@android:style/Theme.Dialog"
            android:windowSoftInputMode="stateAlwaysVisible"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.INPUTACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidtest.austin.LevelSelectActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.LEVELSELECT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.androidtest.austin.Preferences"
		    android:label="@string/app_name"
		    android:screenOrientation="portrait">
			<intent-filter>
  				<action android:name="com.androidtest.austin.PREFERENCES" /> 
  				<category android:name="android.intent.category.DEFAULT" /> 
  			</intent-filter>
  		</activity>
        <activity
            android:name="com.androidtest.austin.FinishedActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.FINISHEDACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidtest.austin.LevelSelectPage"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.androidtest.austin.LPAGE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
